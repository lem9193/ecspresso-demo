{
  "name": "ecspresso-demo",
  "version": "0.1.0",
  "bin": {
    "ecspresso-demo": "bin/ecspresso-demo.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "cdk:diff": "cdk diff --all --no-change-set",
    "cdk:deploy": "cdk deploy --all --require-approval never",
    "cdk:destroy": "cdk destroy --all",
    "format": "prettier --check lib bin",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "aws-cdk": "2.171.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.171.1",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
